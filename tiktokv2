def get_ip_address():
    # T·∫°o m·ªôt socket
    s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)

    try:
        # K·∫øt n·ªëi socket ƒë·∫øn m·ªôt ƒë·ªãa ch·ªâ IP kh√¥ng t·ªìn t·∫°i ƒë·ªÉ l·∫•y IP c·ªßa m√°y hi·ªán t·∫°i
        s.connect(("10.255.255.255", 1))
        ip_address = s.getsockname()[0]
    except Exception:
        # N·∫øu kh√¥ng th·ªÉ k·∫øt n·ªëi, s·ª≠ d·ª•ng ƒë·ªãa ch·ªâ loopback
        ip_address = "127.0.0.1"
    finally:
        s.close()

    return ip_address
